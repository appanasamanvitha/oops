#include<iostream>
using namespace std;
int main(){
        int sum = 0,n,i;
        int *a = new int[n];
        cout<<"ENTER n : ";
        cin>>n;
        for(i=0;i<n;i++){
                cin>>a[i];
        }
        for(i=0;i<n;i++){
                sum += a[i];
        }
        cout<<"sum : "<<sum;
        return 0;
}
